{
   "name": "@mindmapppt/mindmapppt",
   "version": "1.0.0",
   "keywords": [],
   "license": "ISC",
   "author": "",
   "scripts": {
      "build": "next build",
      "clean:temp": "rm -r `ls -d playground-xmind-temp-mmdata/temp-*` && rm -r `ls -d playground-xmind-temp-files/temp-*` && rm -r `ls -d playground-xmind-temp-parsed/temp-*`",
      "clean": "rimraf .next node_modules .turbo .vscode",
      "copy": "pnpm copy:reveal-scripts && pnpm copy:impress-scripts",
      "copy:reveal-scripts": "cp ./node_modules/@mindmapppt/reveal-scripts/bundle.js ./public/reveal-scripts.js",
      "copy:impress-scripts": "cp ./node_modules/@mindmapppt/impress-scripts/bundle.js ./public/impress-scripts.js",
      "dev:next": "next dev",
      "open:browser": "open http://localhost:3000",
      "serve": "pnpm open:browser && serve . -l 6688",
      "dev": "concurrently \"npm:dev:next\" \"npm:serve\"",
      "lint": "next lint",
      "start": "next start -p 3000"
   },
   "dependencies": {
      "@mindmapppt/impress-scripts": "workspace:^1.0.0",
      "@mindmapppt/mmdata-to-impress": "workspace:^1.0.0",
      "@mindmapppt/mmdata-to-reveal": "workspace:^1.0.0",
      "@mindmapppt/reveal-scripts": "workspace:^1.0.0",
      "@mindmapppt/utils": "workspace:^1.0.0",
      "@mindmapppt/xmind-to-mmdata": "workspace:^1.0.0",
      "axios": "^1.2.0",
      "clsx": "^1.2.1",
      "formidable": "^2.0.1",
      "fs-extra": "^10.1.0",
      "next": "^13.0.7",
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
   },
   "devDependencies": {
      "@mapbox/rehype-prism": "^0.6.0",
      "@next/mdx": "^12.0.8",
      "@tailwindcss/forms": "^0.5.3",
      "@tailwindcss/typography": "^0.5.8",
      "@types/formidable": "^2.0.5",
      "@types/fs-extra": "^9.0.13",
      "@types/react": "^18.0.25",
      "autoprefixer": "^10.4.13",
      "concurrently": "^7.6.0",
      "eslint-config-next": "^13.0.5",
      "postcss": "^8.4.19",
      "rehype-join-line": "^1.0.2",
      "serve": "^14.1.2",
      "tailwindcss": "^3.2.4"
   }
}
